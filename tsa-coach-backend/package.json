{
  "name": "tsa-llc-incorporation-cdk",
  "version": "1.0.0",
  "description": "LLC Incorporation Docker Lambda deployment using AWS CDK",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "deploy:dev": "cdk deploy --context stage=dev",
    "deploy:staging": "cdk deploy --context stage=staging",
    "deploy:prod": "cdk deploy --context stage=prod",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "bootstrap": "cdk bootstrap",
    "destroy": "cdk destroy",
    "list": "cdk list",
    "clean": "rm -rf node_modules package-lock.json cdk.out",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "aws-cdk-lib": "2.70.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/tsa-llc-incorporation"
  },
  "keywords": [
    "aws-cdk",
    "lambda",
    "docker",
    "playwright",
    "llc-incorporation",
    "texas-sports-academy"
  ],
  "author": "Texas Sports Academy",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
} 